                            <li>
                                <asp:LoginStatus Style="float: right; margin-left: 10px;" runat="server" ID="logStatus"
                                    LoginText="Accedi" LogoutText="Esci" LogoutPageUrl="~/Design/Catalogo.aspx" CssClass="loginsubmit" />
                            </li>
                        </LoggedInTemplate>
                        <AnonymousTemplate>
                            <li><a href="<%= Page.ResolveClientUrl("~/Design/") %>Accedi.aspx">Accedi / Registrati</a></li>
                        </AnonymousTemplate>
                    </asp:LoginView>
                </ul>
                <div id="container">
